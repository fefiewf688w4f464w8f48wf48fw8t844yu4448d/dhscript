MainTab:addLabel("Pickup Stuff")
MainTab:addButton("Pickup Knives", function()
    for i, v in pairs(game:GetService("Workspace").Ignored.ItemsDrop:GetChildren()) do
        if v:IsA("Part") and v:FindFirstChild("[Knife]") then
            firetouchinterest(game.Players.LocalPlayer.Character.Head, v["[Knife]"].Handle, 0)
        end
    end
end)
MainTab:addButton("Pickup Lockpicks", function()
    for i, v in pairs(game:GetService("Workspace").Ignored.ItemsDrop:GetChildren()) do
        if v:IsA("Part") and v:FindFirstChild("[LockPicker]") then
            firetouchinterest(game.Players.LocalPlayer.Character.Head, v["[LockPicker]"].Handle, 0)
        end
    end
end)
