getgenv().IsOrixonVerified = true

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/fefiewf688w4f464w8f48wf48fw8t844yu4448d/Orixon/main/UI%20Library"))()
local UI = lib:Create("Orixon")

local MainTab = UI:addTab("Main")
local CombatTab = UI:addTab("Combat")
local TargetTab = UI:addTab("Target")
local VisualsTab = UI:addTab("Visuals")
local SettingsTab = UI:addTab("Settings")

getgenv().CircleRadius = 100
getgenv().Prediction = 0.165
getgenv().AimLock = false
getgenv().FovCircle = false
getgenv().UiToggleBind = nil
getgenv().Speed = false
getgenv().FlySpeed = 0.25
getgenv().LockTarget = nil
getgenv().Name = false
getgenv().Line = false
getgenv().Noclip = false
getgenv().TargetPerson = nil
getgenv().AutoKill = false
getgenv().Fling = false
getgenv().Fly = false
getgenv().SelectTarget = false

local aimlockpart = Instance.new("Part", workspace)
aimlockpart.Anchored = true
aimlockpart.Transparency = 1
aimlockpart.CanCollide = false

MainTab:addLabel("Movement")
MainTab:addToggle("Fly", false, function()
    if getgenv().Fly == false then
        getgenv().Fly = true
        local BG = Instance.new('BodyGyro')
		local BV = Instance.new('BodyVelocity')
		BG.P = 9e4
		BG.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
		BV.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
		BG.maxTorque = Vector3.new(math.huge, math.huge, math.huge)
		BG.cframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		BV.velocity = Vector3.new(0, 0, 0)
		BV.maxForce = Vector3.new(math.huge, math.huge, math.huge)
		game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
    else
        getgenv().Fly = false
        game.Players.LocalPlayer.Character.HumanoidRootPart.BodyGyro:Destroy()
        game.Players.LocalPlayer.Character.HumanoidRootPart.BodyVelocity:Destroy()
        game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
    end
end)
MainTab:addSlider("Fly Speed", 10, 0.25, function(v)
    getgenv().FlySpeed = v
end)
MainTab:addToggle("Speed", false, function()
    if getgenv().Speed == false then
        getgenv().Speed = true
    else
        getgenv().Speed = false
    end
    if getgenv().Speed == true then
        Speed()
    end
end)
MainTab:addToggle("Noclip", false, function()
	if getgenv().Noclip == false then
		getgenv().Noclip = true
	else
		getgenv().Noclip = false
	end

	if getgenv().Noclip == true then
		Noclip()
	else
		x:Disconnect()
	end
end)
